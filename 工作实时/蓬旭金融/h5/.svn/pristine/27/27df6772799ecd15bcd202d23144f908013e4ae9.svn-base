<script type="text/javascript">
  var elem = document.getElementById('banner');
  window.mySwipe = Swipe(elem, {
  
 auto: 3000,
 continuous: true,
 disableScroll: true,
 stopPropagation: true,
 callback: function(index, element) {
          $(".btnList li").eq(index).addClass("cur").siblings().removeClass("cur");
        }

});

var tab=document.getElementById("tab");
var spans=tab.getElementsByTagName("span");
var producebox=document.getElementById("producebox");
var uls=producebox.getElementsByTagName("ul");

for(var i=0;i<spans.length;i++){
    spans[i].index=i;
    spans[i].addEventListener("touchend",function(){
        for(var i=0;i<spans.length;i++){
           spans[i].className="";
           uls[i].style.display="none";
        }
        this.className="active";
        uls[this.index].style.display="block";
    })
}



var now=0;
var timer=null;
var msglist=$("#msglist");
var lis=msglist.find("li");

timer=setInterval(function(){
  if(now<lis.length-1){
     now++;
  }else{
    now=0;
  }
  msglist.animate({"top":-now*0.7+"rem"},400);
},2000)

















</script>	
