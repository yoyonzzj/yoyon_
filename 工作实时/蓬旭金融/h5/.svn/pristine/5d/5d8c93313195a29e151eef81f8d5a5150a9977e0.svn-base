//行业资讯选项卡
		var hybtnss=$(".hybtn");
	    hybtnss.mouseover(function(){
	    	hybtnss.css({
	    		borderBottom:"none",
	    		color:"#333"
	    	}).filter(this).css({
	    		borderBottom:"2px solid #ffa540",
	    		color:"#ffa540"
	    	})
	    	var index=$(this).index();
	    	$(".hycontent").css("z-index","1").eq(index).css("z-index","0");
	    })

//媒体报道小轮播
		var mtbtns=$(".mtbtn");
	    var mt=setInterval(mtmove,2000)
	    var mtnow=0;
	    var mtnext=0;
	    var mtz=10;
	    function mtmove(){
	        mtnext++;
	        if(mtnext==mtbtns.length){
	            mtnext=0;
	        }
	        mtbtns.eq(mtnow).css("background-color","#fff").end().eq(mtnext).css("background-color","red");
	        $(".mtitem").eq(mtnext).css({left:330,zIndex:mtz++}).animate({left:0},1000).end().eq(mtnow).animate({left:-330},1000);
	        mtnow=mtnext;
	    }
	    $(".mtpiclunbo").hover(function(){clearInterval(mt)},function(){mt=setInterval(mtmove,2000)})
	    mtbtns.mouseover(function(){
	        mtnext=$(this).index();
	        if(mtnext>mtnow){
	            var l=330;
	        }else if(mtnext<mtnow){
	            var l=-330;
	        }
	        mtbtns.eq(mtnow).css("background-color","#fff").end().eq(mtnext).css("background-color","red");
	        $(".mtitem").eq(mtnext).css({left:l,zIndex:mtz++}).animate({left:0},1000).end().eq(mtnow).animate({left:-l},1000);
	        mtnow=mtnext;
	    })
	    
//行业资讯小轮播
		var hybtns=$(".hybtns");
	    var ht=setInterval(hymove,2000)
	    var hynow=0;
	    var hynext=0;
	    var hyz=10;
	    function hymove(){
	        hynext++;
	        if(hynext==hybtns.length){
	            hynext=0;
	        }
	        hybtns.eq(hynow).css("background-color","#fff").end().eq(hynext).css("background-color","red");
	        $(".hyitem").eq(hynext).css({left:330,zIndex:hyz++}).animate({left:0},1000).end().eq(hynow).animate({left:-330},1000);
	        hynow=hynext;
	    }
	    $(".hypiclunbo").hover(function(){clearInterval(ht)},function(){ht=setInterval(hymove,2000)})
	    hybtns.mouseover(function(){
	        hynext=$(this).index();
	        if(hynext>hynow){
	            var l=330;
	        }else if(hynext<hynow){
	            var l=-330;
	        }
	        hybtns.eq(hynow).css("background-color","#fff").end().eq(hynext).css("background-color","red");
