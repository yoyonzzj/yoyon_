//底部合作伙伴无缝轮播
		$(function(){
		var sec6inner=$(".sec6inner")[0];
		var middlesec6=$(".middlesec6")[0];
		t=setInterval(move,2000);
	    var num=0;
	    function move(){
	    	num++;
	    	if(num==21){
	    		num=1;
	    		sec6inner.style.marginLeft=0;
	    	}
	    	if (num==-1){
	    		num=19;
	    		sec6inner.style.marginLeft="-4080px";
	    	};
	    	animate(sec6inner,{marginLeft:-204*num},500)
	    }
	    	middlesec6.onmouseover=function(){
	    		clearInterval(t);
	    	}
	    	middlesec6.onmouseout=function(){
	    		t=setInterval(move,2000);
	    	}
	    var sec6left=$(".sec6left")[0];
	    var sec6right=$(".sec6right")[0];
	    sec6right.onclick=function(){
	    	move();
	    }
	    sec6left.onclick=function(){
	    	num-=2;
	    	move();
	    }
	})
