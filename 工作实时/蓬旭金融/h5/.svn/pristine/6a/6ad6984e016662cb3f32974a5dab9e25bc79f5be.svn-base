body,input,form,p,pre,h1,h2,h3,h4,h5,h6{margin:0;padding:0;}
ul,li,ol,dl,dt,dd{margin:0;padding:0;list-style: none;}
a{text-decoration: none;-webkit-tap-highlight-color:rgba(0,0,0,0);}
input[type="button"],button,a{-webkit-tap-highlight-color:rgba(0,0,0,0);}
img{border: 0;}
body{font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;-webkit-user-select:none;}
input,textarea{outline: none;}
html,body{width: 100%;}

.czz-top{width:76%;height:340px;background:url(../img/top.png) no-repeat center center;padding:220px 0 0 24%}
.czz-center{width:100%;height:1220px;background:url(../img/zx.png)no-repeat bottom;}

.czz-tp{width:1200px;height:1115px;margin:0 auto;padding-top:105px}
.czz-tp img{width:100%;height:1115px}
.czz-footer{width:100%;height:308px;background:#fff}
.czz-jk{width:450px;height:70px;border-radius: 5px;background:#FD9C29;font-size:20px;line-height:70px;text-align: center;border:0;color:#fff;margin:45px 735px 54px 735px}
.czz-fc{width:100%;height:96px;background:#fec47f;padding-top:24px}
.czz-fc p{font-size:17px;line-height: 36px;margin-left:360px;color:#333}
.czz-iphone{width:158px;height:48px;border-radius: 4px;font-size:18px;color:#fff;border:1px solid #fff;text-align:center;line-height: 48px;}
.czz-i{width:16px;height:22px;margin-right:6px;display:inline-block;background:url(../img/iphone.png)no-repeat center center;background-position: 0px 0px;}
.czz-a{width:16px;height:22px;margin-right:6px;display:inline-block;background:url(../img/android.png)no-repeat center center;}
.czz-az{width:158px;height:48px;border-radius: 4px;font-size:18px;color:#ffd5a3;border:1px solid #ffd5a3;text-align:center;line-height: 48px;margin-top:20px}
.czz-iphone:hover{width:158px;height:48px;border-radius:4px;font-size:18px;color:#ff971b;border:1px solid #fff;text-align:center;line-height: 48px;background:#fff;}
.czz-iphone:hover i{background:url(../img/iphone.png)no-repeat center center;background-position: 0px -25px;}
