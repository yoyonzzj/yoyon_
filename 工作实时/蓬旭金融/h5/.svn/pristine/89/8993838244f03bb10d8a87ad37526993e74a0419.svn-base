/*弹框样式*/
.imgzoom_pack{
	width:100%;
	height:100%;
	position:fixed;
	left:0;
	top:500%;
	background:rgba(0,0,0,.7);
	display:none;
	z-index: 9999;
}
.imgzoom_pack1{
	width:100%;
	height:100%;
	position:fixed;
	left:0;
	top:600%;
	background:rgba(0,0,0,.7);
	display:none;
	z-index: 9999;
}
.imgzoom_pack .imgzoom_x{
	color:#fff;
	height:30px;
	width:30px;
	line-height:30px;
	background:#000;
	text-align:center;
	position:absolute;
	right:5px;
	top:5px;
	z-index:10;
	cursor:pointer;
}
.imgzoom_pack .imgzoom_img{
	width:100%;
	height:80%;
	position:absolute;
	left:0;
	top:10%;
	overflow:hidden;
}
.imgzoom_pack .imgzoom_img img{
	width:100%;
	height:100%;
	position:absolute;
	top:47%;
}
