body{background:rgb(244,244,244)}
section h4{
	height:0.7rem;
	line-height:0.7rem;
	font-size:0.26rem;
	color:rgb(102,102,102);
	font-weight:normal;
	padding-left:0.3rem;
}
section h4 span{
	color:red;
	margin-right:0.1rem;
}
section p{
	height:0.9rem;
	line-height:0.9rem;
	background:#fff;
	font-size:0.3rem;
	color:rgb(153,153,153);
	padding-left:0.4rem;
	border-bottom:1px solid rgb(236,236,236);
}

section p input{
	border:none;
	display:block;
	width:100%;
	height:0.9rem;
	line-height:0.9rem;
	font-size:0.25rem;
	color:rgb(153,153,153);
}
#protocol{
	width:6rem;
	margin:0 auto;
	padding-top:0.25rem;
}
.protocol{
	color:rgb(28,28,28);
	font-size:0.22rem;
	height:0.3rem;
	line-height:0.3rem;
}
.protocol span{
	position:relative;
	float: left;
	padding-left:0.5rem;
	height:0.3rem;
}
.protocol em{
	float: left;
	font-style:normal;
	height:0.3rem;
}
.protocol span i{
  position:absolute;
  left:0rem;
  top:0rem;
  width:0.3rem;
  height:0.3rem;
  background:url(../img/weixuanze.png) no-repeat;
  background-size:cover;
}
.protocol span.active i{
   background:url(../img/xuanze.png) no-repeat;
   background-size:cover;
}
.protocol a{
	float: left;
	color:rgb(255,151,27);
	height:0.3rem;
}
.btn{
	display:block;
	margin:0.8rem auto 0;
	color:#fff;
	width:6rem;
	height:0.88rem;
	line-height:0.88rem;
	background:rgb(241, 156, 63);
	border-radius:0.1rem;
	text-align: center;
	font-size:0.34rem;
}