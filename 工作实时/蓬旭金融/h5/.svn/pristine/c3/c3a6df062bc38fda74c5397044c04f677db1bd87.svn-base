ody,input,form,p,pre,h1,h2,h3,h4,h5,h6{margin:0;padding:0;}
ul,li,ol,dl,dt,dd,img{margin:0;padding:0;list-style: none;}
a{text-decoration: none;-webkit-tap-highlight-color:rgba(0,0,0,0);}
input[type="button"],button,a{-webkit-tap-highlight-color:rgba(0,0,0,0);}
img{border: 0;}
body{font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;-webkit-user-select:none;}
input,textarea{outline: none;}
html,body{width: 100%;height:100%}
*{margin: 0;padding:0}

.swiper-container {width: 100%;height: 100%;}  
.czz-sy{width:100%;height:100%}
.czz-nr{width:100%;height:100%}
.czz-bj{width:100%;height:90%;background:url(../img/content.png) no-repeat center center;background-size:cover}
/*.czz-bj span{width:.6rem;height:.3rem;display:inline-block;margin:140% 39% 0 46%}
.czz-bj span img{width:100%;height:100%}*/
.czz-cn{width:90%;height:85%;padding:0 5% 0 5%}
.czz-bt{font-size:.36rem;line-height: .98rem;text-align: center;color:#ff971b;}
.czz-dl{font-size:.24rem;line-height:.48rem;text-indent: .48rem;color:#989898;}
.czz-footer{width:100%;height:15%;background:url(../img/footer.png) no-repeat center center;background-size:cover;padding-top:8%}
.czz-footer a{color:#ff971b;}
.czz-footer p{width:3.4rem;height:.74rem;line-height: .74rem;font-size:.26rem;background:#fff;border-radius: .5rem;text-align: center;margin:0 auto;}
@-moz-keyframes myMove {
  0% {padding-top:9.6rem;}
  100% {padding-top:9.8rem;}
}
@-webkit-keyframes myMove {
  0% {padding-top:9.6rem;}
  100% {padding-top:9.8rem;}
}
@-o-keyframes myMove {
  0% {padding-top:9.6rem;}
  100% {padding-top:9.8rem;}
}
@keyframes myMove {
  0% {padding-top:9.6rem;}
  100% {padding-top:9.8rem;}
}
#down img{
	margin-left:46%;
  width:.6rem;height:.3rem;
  -moz-animation: myMove 1s linear infinite alternate; 
  -webkit-animation: myMove 1s linear infinite alternate;
  -o-animation: myMove 1s linear infinite alternate;
  animation: myMove 1s linear infinite alternate;
}