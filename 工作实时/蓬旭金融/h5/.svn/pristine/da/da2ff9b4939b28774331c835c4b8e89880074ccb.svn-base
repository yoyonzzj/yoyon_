body,h1,h2,h3,h4,h5,h6,hr,p,blockquote,dl,dt,dd,ul,ol,li,pre,form,fieldset,legend,button,input,textarea,th,td,div{margin:0;padding:0;}
body,button,input,select,textarea{font-family: "微软雅黑","MicroSoft YaHei","Arial Narrow","HELVETICA"; font-size:14px; padding:0; margin:0; color:#666;}
table{border-collapse:collapse;border-spacing:0;}
ul,li{ margin:0px; padding:0px; list-style:none; }
img{border:none;}
h1,h2,h3,h4,h5,h6,p{ margin:0; padding:0;}
.clear{ clear:both; margin:0; padding:0;}
a{text-decoration:none; color:#666;}
html{overflow-x:hidden;-webkit-text-size-adjust:none;}

.czz-top{width:100%;height:500px;background:url(../img/huodong.png) no-repeat center center;}
.czz-main{width:100%;height:auto;background:#6749d2;}
.czz-center{width:1100px;height:1550px;padding-top:50px;margin: 0 auto}
.czz-active1{width:1100px;height:240px;background:url(../img/haoli1.png) no-repeat center center}

.czz-p2{float:right;margin:130px 80px 0 0;width:237px;height:54px;background:#ffe135;line-height: 54px;text-align:center;font-size:26px;color:#987cf6;border-radius: 50px;cursor: pointer;}
.czz-p2:hover{background:#FDB86C}
.czz-active2{width:1100px;height:240px;background:url(../img/haoli2.png);margin-top:30px}
.czz-active3{width:1100px;height:240px;background:url(../img/haoli3.png);margin-top:30px}

.czz-rule{width:1100px;height:620px;background:url(../img/rule.png);margin-top:30px}
.czz-box3{width:900px;height:525px;padding:67px 0 0 120px;}
.czz-box3 ul li{margin-top:20px;}
.czz-box3 ul li p{font-size:17px;color:#fff;line-height: 30px;}
.czz-title{font-size:18px;color:#fff;}
.czz-title img{margin-right:10px}
.czz-dl{margin-left:25px;}
.czz-dl img{margin-right:10px}
#czz-ts{color:#ffe135}
