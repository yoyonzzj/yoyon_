.zhezhao,.zhezhao1,.zhezhao2,.zhezhao3,.zhezhao4{
	position: fixed;
	width: 100%;
	height: 100%;
	display: none;
	background: rgba(0,0,0,.5);
	left: 0;right: 0;top: 0;bottom: 0;
	margin:auto;
	z-index:9999;
}
.tanchuang2{
	width: 500px;
	height: 350px;
	position: absolute;
	left: 0;top: 0;bottom: 0;right: 0;
	margin: auto;
}
.jiaohubox2{
	width: 100%;
	height: 268px;
	-moz-border-radius:20px; /* Gecko browsers */
	-webkit-border-radius: 20px;   /* Webkit browsers */
	border-radius: 20px;/* W3C syntax */
	background: #6749d2;
	padding-top: 10px;
}
.innerbox2{
	width: 480px;
	height: 210px;
	margin: 0 auto;
	/*padding-top: 48px;*/
	-moz-border-radius:20px; /* Gecko browsers */
	-webkit-border-radius: 20px;   /* Webkit browsers */
	border-radius: 20px;/* W3C syntax */
	background: #FFFFFF;
	box-sizing: content-box;
	-moz-box-sizing: content-box; /* Firefox */
	-webkit-box-sizing: content-box; /* Safari */
}
.cancel{
	width: 140px;
	height: 54px;
	background: #BABABA;
	text-align: center;
	line-height: 54px;
	color: #fff;
	font-family: "微软雅黑";
	font-size: 30px;
	float: left;
	margin-left: 70px;
	margin-top: 40px;
	cursor: pointer;
	-moz-border-radius:10px; /* Gecko browsers */
	-webkit-border-radius: 10px;   /* Webkit browsers */
	border-radius: 10px;/* W3C syntax */
}
.login{
	width: 140px;
	height: 54px;
	background: #FFAB14;
	text-align: center;
	line-height: 54px;
	color: #fff;
	font-family: "微软雅黑";
