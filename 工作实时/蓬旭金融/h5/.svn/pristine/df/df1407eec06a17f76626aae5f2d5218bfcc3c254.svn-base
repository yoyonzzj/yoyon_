body{background:rgb(244,244,244)}
section h4{
	height:0.7rem;
	line-height:0.7rem;
	font-size:0.26rem;
	color:rgb(102,102,102);
	font-weight:normal;
	padding-left:0.3rem;
}
section h4 span{
	color:red;
	margin-right:0.1rem;
}
section p{
	height:0.9rem;
	line-height:0.9rem;
	background:#fff;
	font-size:0.3rem;
	color:rgb(153,153,153);
	padding-left:0.4rem;
	border-bottom:1px solid rgb(236,236,236);
}

section p input{
	border:none;
	display:block;
	width:100%;
	height:0.5rem;
	line-height:0.5rem;
	font-size:0.25rem;
	color:rgb(153,153,153);
	padding:0.2rem 0;
}
#protocol{
	width:6rem;
	margin:0 auto;
	padding-top:0.25rem;
}
.protocol{
	color:rgb(28,28,28);
	font-size:0.22rem;
	height:0.3rem;
	line-height:0.3rem;
}
.protocol span{
	position:relative;
	float: left;
	padding-left:0.5rem;
	height:0.3rem;
}
.protocol em{
	float: left;
	font-style:normal;
	height:0.3rem;
}
.protocol span i{
  position:absolute;
  left:0rem;
  top:0rem;
  width:0.3rem;
  height:0.3rem;
  background:url(../img/weixuanze.png) no-repeat;
  background-size:cover;
}
.protocol span.active i{
   background:url(../img/xuanze.png) no-repeat;
   background-size:cover;
}
.protocol a{
	float: left;
	color:rgb(255,151,27);
	height:0.3rem;
}
.btn{
	display:block;
	margin:0.8rem auto 0;
	color:#fff;
	width:6rem;
	height:0.88rem;
	line-height:0.88rem;
	background:rgb(241, 156, 63);
	border-radius:0.1rem;
	text-align: center;
	font-size:0.34rem;
}









#mask{
	position:fixed;
	left:0;
	top:0;
	width:100%;
	height:100%;
	background:#000;
	opacity:0.4;
	display:none;
}
#keyboard{
	position:fixed;
	width:100%;
	left:0;
	bottom:0;
	background:#fff;
	transition:0.5s;
	-webkit-transform:translateY(4rem);
	-moz-transform:translateY(4rem);
	-ms-transform:translateY(4rem);
	transform:translateY(4rem);
}
#keyboard p{
	float: left;
	width:33.33%;
	height:1rem;
	text-align: center;
	border:1px solid #ccc;
	box-sizing:border-box;
	font-size:0.2rem;
	position:relative;
}
#keyboard p.gray{
	background:#ccc;
}
#keyboard p span{
	font-size:0.5rem;
	position:absolute;
	width:100%;
	height:100%;
	left:0;
	top:0;
	padding-top:0rem;
	box-sizing:border-box;
}
#keyboard p span.zero{
	padding-top:0.2rem;
}
#keyboard p em{
	font-style:normal;
	position:absolute;
	left:0;
	top:0;
	width:100%;
	height:100%;
	font-size:0.5rem;
	padding-top:0.2rem;
}
#keyboard p  strong{
    padding-top: 0.3rem;
    display: block;
    font-size:0.2rem;
}

#dialog{
	position:fixed;
	width:5.46rem;
	margin-left:-2.73rem;
	left:50%;
	top:10%;
	background:#fff;
	border-radius:0.1rem;
	display:none;
}
#dialog h4{
	height:0.84rem;
	text-align: center;
	font-weight:normal;
	font-size:0.32rem;
	line-height:0.84rem;
	border-bottom:1px solid rgb(220,220,220);
}

#dialog em{
	font-style:normal;
	text-align: center;
	display:block;
	font-size:0.28rem;
	line-height:0.83rem;
	color:rgb(102,102,102);
}
#dialog strong{
	display:block;
	text-align: center;
	font-weight:normal;
	font-size:0.48rem;
}
#dialog p{
	color:rgb(153,153,153);
	font-size:0.24rem;
	padding-left:0.2rem;
}
#dialog ul{
	border:1px solid rgb(220,220,220);
	height:0.8rem;
	width:4.8rem;
	border-right:none;
	margin: 0.4rem auto;
}
#dialog li{
   width:0.8rem;
   height:0.8rem;
   line-height:0.8rem;
   text-align: center;
   float: left;
   box-sizing:border-box;
   border-right:1px solid rgb(220,220,220);
}
#send{
	border:none;
	outline:none;
	display:block;
	width:1.7rem;
	height:0.58rem;
	background:rgb(255,151,27);
	color:#fff;
	border-radius:0.1rem;
	margin:0.39rem auto;
	font-size:0.3rem;
}
#close{
	position:absolute;
	right:0.3rem;
	top:0.3rem;
	width:0.3rem;
	height:0.3rem;
	background:url(../img/close1.png) no-repeat;
	background-size:cover;
}