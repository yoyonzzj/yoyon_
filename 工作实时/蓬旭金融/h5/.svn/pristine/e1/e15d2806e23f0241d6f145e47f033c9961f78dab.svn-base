body{background:rgb(244,244,244)}
dl{
	height:1.58rem;
	background:#fff;
    padding:0.4rem 0 0 0.7rem;
    box-sizing:border-box;
}
dl dt{
	width:0.88rem;
	height:0.88rem;
	background:red;
	float: left;
	border-radius:50%;
	margin-right:0.63rem;
}
dl dd h4{
	font-weight:normal;
	color:rgb(102,102,102);
	font-size:0.3rem;
	padding-bottom:0.1rem;
}
dl dd p{
	font-size:0.3rem;
	color: rgb(153,153,153);
}

.txt{
    margin-top:0.18rem;
	background:#fff;
	padding-left:0.3rem;
}
.txt p{
	overflow:hidden;
	height:0.7rem;
	line-height:0.7rem; 
}
.txt span{
    font-size:0.28rem;
	color:rgb(102,102,102);
	display:block;
	line-height:0.7rem;
}
.txt em{
	font-style:normal;
	float: left;
	color:rgb(102,102,102);
	font-size:0.6rem;

}
.txt input{
	border:none;
	outline:none;
	float: left;
	background:none;
	width:5rem;
	font-size:0.28rem;
	height:0.7rem;
	line-height:0.7rem;
    padding-left:0.1rem;
}
.msg{
	line-height:0.7rem;
	font-size:0.26rem;
	color:rgb(204,204,204);
	background:#fff;
	border-top:1px solid rgb(236,236,236);
	padding-left:0.3rem;
}
.msg span{
	margin:0 0.1rem;
}
.msg em{
	float: right;
	font-style:normal;
	font-size:0.26rem;
	margin-right:0.3rem;
	color:rgb(255,151,27);
}

#btn{
	display:block;
	width:6rem;
	height:0.88rem;
	/* background:rgb(255,151,27); */
	border:none;
	outline:none;
	color:#fff;
	font-size:0.34rem;
	margin:1.6rem auto;
	border-radius:0.1rem;
	background: rgb(245, 207, 162);
}

#btn1{
	display:block;
	margin:0 auto;
	width:1.7rem;
	height:0.6rem;
	text-align: center;
	line-height: 0.6rem;
	color:#fff;
	background:rgb(255,151,27);
	border-radius:0.1rem;
}

#pwd{
	display:block;
	width:4.84rem;
	height:0.8rem;
	line-height:0.8rem;
	padding-left:0.1rem;
	box-sizing:border-box;
	border:1px solid rgb(220,220,220);
	margin:0.3rem auto;
}
#close{
	position:absolute;
	width:0.3rem;
	height:0.3rem;
	right:0.3rem;
	top:0.3rem;
	background: url(../img/close1.png) no-repeat;
	background-size:cover;
}








#mask{
	position:fixed;
	left:0;
	top:0;
	width:100%;
	height:100%;
	background:#000;
	opacity:0.4;
	display:none;
}


#dialog{
	position:fixed;
	width:5.46rem;
	margin-left:-2.73rem;
	left:50%;
	top:10%;
	background:#fff;
	border-radius:0.1rem;
	display:none;
	padding-bottom:0.4rem;
}
#dialog h4{
	height:0.84rem;
	text-align: center;
	font-weight:normal;
	font-size:0.32rem;
	line-height:0.84rem;
	border-bottom:1px solid rgb(220,220,220);
}

#dialog em{
	font-style:normal;
	text-align: center;
	display:block;
	font-size:0.28rem;
	line-height:0.83rem;
	color:rgb(102,102,102);
}
#dialog strong{
	display:block;
	text-align: center;
	font-weight:normal;
	font-size:0.48rem;
}
#dialog p{
	color:rgb(153,153,153);
	font-size:0.24rem;
	padding-left:0.2rem;
}

#send{
	border:none;
	outline:none;
	display:block;
	width:1.7rem;
	height:0.58rem;
	background:rgb(255,151,27);
	color:#fff;
	border-radius:0.1rem;
	margin:0.39rem auto;
	font-size:0.3rem;
}