.czz-container{width:1920px;height:auto;background:#f4f4f4;margin:0 auto}
.czz-conter{width:1200px;height:auto;margin:0 auto;padding-top: 18px;}
.czz-nav{width: 1200px;height: 60px;background-color: #ffffff;border: solid 1px #ececec;margin-bottom:18px}
.czz-list{margin-left:15px;}
.czz-list li{float:left;width:120px;height:60px;font-size: 18px;font-weight: 300;text-align:center;line-height: 60px;letter-spacing: 0.4px;}
.czz-ys{border-bottom: 2px solid #ffa540;color:#ffa540;cursor: pointer;}


.czz-chanpin{width:1120px;height:auto;padding: 0 40px 0 40px;background-color: #fff;}
