.czz-lis4{margin:10px 0 0 90px}
.czz-lis4 p:nth-of-type(1){font-weight: 300;letter-spacing: 0.5px; color: #ff4e04;}
.czz-lis4 P:nth-of-type(2){margin-top:10px;font-size: 14px;font-weight: 300;letter-spacing: 0.3px;color: #666;}
.czz-lis5{margin:24px 0 0 118px}
