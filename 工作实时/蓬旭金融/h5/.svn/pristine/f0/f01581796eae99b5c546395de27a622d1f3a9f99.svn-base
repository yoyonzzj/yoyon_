<script type="text/javascript">
  var prev=document.getElementById("prev");
  var next=document.getElementById("next");
  var salenum=document.getElementById("salenum");
  var resultprice=document.getElementById("resultprice");

  salenum.addEventListener("input",function(){
      if(this.value==""){
        this.value=1
      }
      var num=parseInt(salenum.value);
       resultprice.innerHTML=num.toFixed(2)
  })



  prev.addEventListener("touchend",function(){
     var num=parseInt(salenum.value);
     if(num>1){
        num--;
     }
     salenum.value=num;
     
     resultprice.innerHTML=num.toFixed(2)
  })


 next.addEventListener("touchend",function(){
     var num=parseInt(salenum.value);
     num++
     salenum.value=num;
     resultprice.innerHTML=num.toFixed(2)
  })






</script>

