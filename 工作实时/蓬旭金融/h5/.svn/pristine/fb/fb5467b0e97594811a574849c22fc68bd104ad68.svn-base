body,input,form,p,pre,h1,h2,h3,h4,h5,h6{margin:0;padding:0;}
ul,li,ol,dl,dt,dd{margin:0;padding:0;list-style: none;}
a{text-decoration: none;-webkit-tap-highlight-color:rgba(0,0,0,0);}
input[type="button"],button,a{-webkit-tap-highlight-color:rgba(0,0,0,0);}
img{border: 0;}
body{font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;-webkit-user-select:none;}
input,textarea{outline: none;}
html,body{width: 100%;}

.czz-top{width:100%;height:17vh;}
.czz-top img{width:100%;height:100%}
.czz-body{width:100%;height:auto;background: #F4F4F4;}
.czz-nav{width:100%;height:32vh;background:#fff}
.czz-nav ul li{float:left;width:1rem;margin:.1rem .56rem 0 .56rem}
.czz-nav ul li img{width:.48rem;height:.48rem;margin-left:.24rem}
.czz-nav ul li p{font-size:.24rem;color:#666;text-align: center;}
#czz-news{width:100%;height:24vh;margin-top:.1rem;background:#fff}
#czz-news div{display:none}
#czz-news ul{width:100%;height:.88rem;border-bottom: .01rem solid #DCDCDC;}
#czz-news ul li{font-size:.26rem;line-height:.88rem;width:1.3rem;text-align:center;height:.88rem;float:left;margin-left:.2rem}
.czz-more{font-size:0.22rem;width:1.5rem;height:.88rem;line-height:.88rem;text-align: center;float:right;border-bottom: 0;margin-right:.2rem;display:inline-block}
.czz-more a{color:#999;}
._li01{border-bottom:.01rem solid #FF971B;color:#FF971B;}
._div{display:block !important;width:100%;height:auto}
.czz-headline{font-size:.28rem;width:100%;height:.76rem;line-height: .76rem;margin:.24rem 0 0 .4rem;color:#666;}
.czz-txt{font-size:.26rem;width:5.8rem;height:.4rem;color:#999;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;margin:0 0 .46rem .4rem}
.czz-x{width:100%;height:.01rem;border-bottom: .01rem solid #ECECEC;}
