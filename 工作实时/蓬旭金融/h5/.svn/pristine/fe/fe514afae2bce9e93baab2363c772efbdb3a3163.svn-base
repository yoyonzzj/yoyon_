<script type="text/javascript">
var elem = document.getElementById('banner');
  window.mySwipe = Swipe(elem, {
  
 auto: 3000,
 continuous: true,
 disableScroll: true,
 stopPropagation: true,
 callback: function(index, element) {
          $(".btnList li").eq(index).addClass("cur").siblings().removeClass("cur");
        }

});


















//点击购买次数功能
  var prev=document.getElementById("prev");
  var next=document.getElementById("next");
  var salenum=document.getElementById("salenum");
  var resultprice=document.getElementById("resultprice");


  //剩余次数英文
  var remanent=parseInt(document.getElementById("remanent").innerHTML);
  salenum.addEventListener("input",function(){
      
      if(this.value=="" || this.value==0){
         this.value=1
      }else if(parseInt(salenum.value)>remanent){
        this.value=remanent;
      }
       
       var num=parseInt(salenum.value);

       resultprice.innerHTML=num.toFixed(2)
  
  })


 prev.addEventListener("touchend",function(){
     var num=parseInt(salenum.value);
     if(num>1){
        num--;
     }
     salenum.value=num;
     
     resultprice.innerHTML=num.toFixed(2)
  })


 next.addEventListener("touchend",function(){
     var num=parseInt(salenum.value);
     num++
     salenum.value=num;
     resultprice.innerHTML=num.toFixed(2)
  })
















//点击查看更多幸运号码弹窗效果
var more=document.getElementById("more");
var mask=document.getElementById("mask");
var dialog=document.getElementById("dialog");
var close=document.getElementById("close");
more.addEventListener("touchend",function(){
   mask.style.display=dialog.style.display="block";

})

close.addEventListener("touchend",function(){
   mask.style.display=dialog.style.display="none";

})

//是否现实查看更多按钮
var luckynumber=document.getElementById("luckynumber");
var numbers=luckynumber.getElementsByTagName("span");
if(numbers.length>=2){
   more.style.display="block";
}else{
   more.style.display="none";
}



//倒计时
function toZero(num){
   if(num>9){
    return num
   }else{
    return "0"+num
   }
}

var timer=null;
function ShowCountDown(year,month,day,h,m,s,divname){ 
      var iNow=new Date(); 
      var iNew=new Date(year, month-1, day, h, m, s); 
      var t=Math.floor((iNew-iNow)/1000);
      var day=Math.floor(t/86400);
      var h=Math.floor(t%86400/3600)+day*24;
      var m=Math.floor(t%86400%3600/60);
      var s=t%60
      var str=toZero(h)+":"+toZero(m)+":"+toZero(s);
      var time=document.getElementById(divname); 
      time.innerHTML = str; 
      if(h==0 && m==0 && s==0){
        clearInterval(timer);
      }
} 
  
  
ShowCountDown(2017,08,08,11,39,60,'countdown');
timer=setInterval(function(){
    ShowCountDown(2017,08,08,11,39,60,'countdown');
},1000); 


