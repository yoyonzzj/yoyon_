*{margin:0;padding:0;}
a:link,a:visited{
	text-decoration:none;
	list-style:none;
}
a{
    -webkit-tap-highlight-color:rgba(0,0,0,0);
    tap-highlight-color:rgba(0,0,0,0);
    -webkit-tap-highlight-color: transparent;
    tap-highlight-color: transparent;
}
li{list-style:none;}
html,body{width:100%;font-family:"微软雅黑";font-size:0.24rem;}
#container{
   width:100%;
   height:30rem;
   background:url(../images/bg.png) no-repeat;
   background-size:cover;
}
.content img{
	width:100%;
	display:block;
}
.content{
	padding-top:0.17rem;
}
.content1{
	width:6.5rem;
	height:10.39rem;
	margin:-1.52rem auto 0;
	background:url(../images/content1.png) no-repeat;
	background-size:cover;
	position:relative;
}
.content1 ul{
	padding-top:1.52rem;
}
.content1 ul li{
	margin:0 auto 0.34rem;
	width:5.84rem;
	height:2.54rem;
}
.content1 ul li a{
	float: left;
	width:2.54rem;
	height:0.86rem;
	background:url(../images/btn.png) no-repeat;
	background-size:cover;
	color:#fff;
	text-align: center;
	line-height:0.78rem;
	font-size:0.28rem;
	margin:1.45rem 0rem 0rem 1.79rem; 
}
.content1 ul li a.active{
	background:url(../images/btn_hover.png) no-repeat;
	background-size:cover;
}
.content1 ul li:nth-of-type(1){
	background:url(../images/1.png) no-repeat;
	background-size:cover;
}
.content1 ul li:nth-of-type(2){
	background:url(../images/2.png) no-repeat;
	background-size:cover;
}
.content1 ul li:nth-of-type(3){
	background:url(../images/3.png) no-repeat;
	background-size:cover;
}
.content2{
	width:6.5rem;
	height:10.99rem;
	margin:0.3rem auto 0;
	background:url(../images/content2.png) no-repeat;
	background-size:cover;
	position:relative;
	color:#e587be;
	padding-top:1.2rem;
	box-sizing:border-box;
}
.content2 ul{
   padding:0 0.3rem 0.3rem;
}
.content2 p{
	padding:0 0.3rem;
}
.content2 ul li{
  padding-bottom:0.2rem;
  line-height:0.35rem;
}
.footer{
	text-align: center;
	color:#afd3e9;
	padding-top:0.35rem;
}